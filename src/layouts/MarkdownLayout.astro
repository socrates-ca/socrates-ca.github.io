---
import DefaultLayout from './DefaultLayout.astro'

interface Props {
  frontmatter: {
    title: string
  }
}

const { frontmatter } = Astro.props
---

<DefaultLayout title={frontmatter.title}>
  <div class="container">
    <div class="space-content my-12">
      <slot />
    </div>
  </div>
</DefaultLayout>
